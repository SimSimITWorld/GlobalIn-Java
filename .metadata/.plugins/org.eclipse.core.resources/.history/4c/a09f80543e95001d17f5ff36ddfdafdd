package newprac;

/* 문]
 *     2차원 배열을 이용하여 다음과 같이 출력하시오.
 *     
 *     1
 *     1 2 3
 *     1
 *     1 2 3 4
 *     1 2
 */

import java.util.*;

public class Prac {

	public static void main(String[] args) {

		int[][] a = 
			{
					{1},
					{1, 2, 3},
					{1},
					{1, 2, 3, 4},
					{1, 2}
			};
		
		for(int i = 0 ; i < a.length;i++) {
			for(int j = 0;j<a[i].length;j++) {
				System.out.print(a[i][j] + " ");
			}
			System.out.println();
		}
		
	}

}
