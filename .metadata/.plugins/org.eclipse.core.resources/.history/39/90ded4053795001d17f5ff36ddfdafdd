package newprac;

/* 문]
 *     5개의 정수를 입력받고 그 중 양수들만의 합계를 출력하는 프로그램 작성
 *     단, 0이나 음수가 입력되면 continue 문을 이용하여 넘어간다.
 */

import java.util.*;

public class Prac {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int sum = 0;
		
		for(int i = 0;i<5;i++) {
			System.out.print("값 입력 : ");
			int a = sc.nextInt();
			if(a>0)
				sum+=a;
			else
				continue;
		}
		
		
	}

}
